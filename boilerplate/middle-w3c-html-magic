  <script>
   function getCookie(name) {
     var params = location.search.substr(1).split("&");
     for (var index = 0; index < params.length; index++) {
       if (params[index] == name)
         return "1";
       var data = params[index].split("=");
       if (data[0] == name)
         return unescape(data[1]);
     }
     var cookies = document.cookie.split("; ");
     for (var index = 0; index < cookies.length; index++) {
       var data = cookies[index].split("=");
       if (data[0] == name)
         return unescape(data[1]);
     }
     return null;
   }
  </script>
  <!-- style switcher script by Stommepoes  -->
  <link title="Author documentation only" media="screen,projection" rel="alternate stylesheet" id="authors" href="switcher/teststylesheet.css">
  <script src="switcher/authorstylesheet.js" type="text/javascript"></script>
